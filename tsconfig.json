{
	"compilerOptions": {
		"module": "esnext",
		"moduleResolution": "nodenext",
		"target": "es2017",
		"allowImportingTsExtensions": true,
		"esModuleInterop": true,
		"outDir": "build",
		"lib": ["es2019", "dom", "dom.iterable"],
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"preserveConstEnums": true,
		"removeComments": true,
		"sourceMap": true,
		"strict": true,
		"allowJs": true,
		"baseUrl": ".",
		"importHelpers": true
	}
}
